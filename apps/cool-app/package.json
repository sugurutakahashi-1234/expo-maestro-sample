{
  "name": "cool-app",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "echo 'No tests configured for cool-app'",
    "get-ios-device": "xcrun simctl list devices booted | grep -o '[A-F0-9-]\\{36\\}' | head -1",
    "get-android-device": "adb devices | grep -v 'List' | grep 'device$' | awk '{print $1}' | head -1",
    "test:maestro:ios": "DEVICE_ID=$(bun run get-ios-device) && maestro --device $DEVICE_ID test --config .maestro/config-ios.yaml .maestro/cool-app-flow.yaml",
    "test:maestro:android": "DEVICE_ID=$(bun run get-android-device) && maestro --device $DEVICE_ID test --config .maestro/config-android.yaml .maestro/cool-app-flow.yaml",
    "build:android:release": "npx expo run:android --variant release",
    "build:ios:release": "npx expo run:ios --configuration Release",
    "test:maestro:android:release": "bun run build:android:release && DEVICE_ID=$(bun run get-android-device) && maestro --device $DEVICE_ID test --config .maestro/config-android.yaml .maestro/cool-app-flow.yaml",
    "test:maestro:ios:release": "bun run build:ios:release && DEVICE_ID=$(bun run get-ios-device) && maestro --device $DEVICE_ID test --config .maestro/config-ios.yaml .maestro/cool-app-flow.yaml",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "format": "echo 'No formatter configured for cool-app'",
    "clean": "rm -rf node_modules .expo",
    "ci": "bun run typecheck && bun run lint && bun run test"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "cool-package": "*",
    "expo": "~54.0.23",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native": "catalog:",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-worklets": "0.5.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "catalog:"
  },
  "private": true
}
